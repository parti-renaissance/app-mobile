  "private": true,
  "scripts": {
    "deplint": "depcruise --config .dependency-cruiser.js src",
    "emulator": "emulator -list-avds | head -n1 | xargs emulator -avd",
    "eslint": "eslint . --ext js,ts,tsx --max-warnings 0",
    "eslint:write": "yarn eslint --fix",
    "jest": "jest",
    "open:android": "open -a \"/Applications/Android Studio.app\" android",
    "open:ios": "open \"ios/JeMarche.xcworkspace\"",
    "pod:install": "cd ios && bundle exec pod install && cd ..",
    "preflight": "yarn tsc && yarn eslint && yarn deplint",
    "run:android": "react-native run-android --variant=stagingDebug --appId fr.en_marche.jecoute.staging",
    "run:ios": "react-native run-ios --simulator \"iPhone 14\"",
    "simulator": "open -a Simulator",
    "start": "react-native start",
    "tsc": "tsc --build",
    "tsc:watch": "yarn tsc --watch",
    "postinstall": "patch-package && npx jetify"
  },
